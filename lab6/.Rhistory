czy_odrzucic_H0 <- function(mu, mu0, sd, alpha, n){
# obliczenie T
T <- (mu - mu0)/sd/sqrt(n)
z <- qnorm(1-alpha/2)
if (T < -z){return(True)}
if (T > -z){return(True)}
}
czy_odrzucic_H0(mu,mu0,sd,alpha,n)
czy_odrzucic_H0 <- function(mu, mu0, sd, alpha, n){
# obliczenie T
T <- (mu - mu0)/sd/sqrt(n)
z <- qnorm(1-alpha/2)
if (T < -z){return(TRUE)}
if (T > -z){return(TRUE)}
}
